{
  "name": "random-html-id",
  "version": "1.0.0",
  "description": "A HTML5 unique id generator.",
  "main": "dist/index.min.js",
  "files": [
    "dist/index.min.js",
    "dist/index.d.ts"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node-dev --respawn src/index.ts",
    "build": "rmdir /s /Q dist & tsc && terser dist/index.js --output dist/index.min.js --config_file terserconfig.json --comments /^\\*\\n/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joker876/random-id.git"
  },
  "bugs": {
    "url": "https://github.com/joker876/random-id/issues"
  },
  "homepage": "https://github.com/joker876/random-id#readme",
  "keywords": [
    "id",
    "html5",
    "html",
    "unique",
    "id",
    "uuid"
  ],
  "author": "joker876",
  "license": "MIT",
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "short-uuid": "^4.2.0",
    "take-chance": "^4.3.2"
  }
}
